# Streamlit Secrets Template
# Copy this file to secrets.toml and fill in your actual values
# DO NOT commit secrets.toml to version control

[secrets]
# OpenAI API Key for AI processing (GPT-4o-mini)
OPENAI_API_KEY = "your_openai_api_key_here"

# AWS Credentials for Textract document processing
AWS_ACCESS_KEY_ID = "your_aws_access_key_here"
AWS_SECRET_ACCESS_KEY = "your_aws_secret_access_key_here"
AWS_DEFAULT_REGION = "ap-south-1"

# Optional: Google Sheets integration
GOOGLE_SHEETS_CREDENTIALS_JSON = """
{
  "type": "service_account",
  "project_id": "your_project_id",
  // ... rest of your Google service account JSON
}
"""